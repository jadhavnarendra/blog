<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= stylesheet_link_tag "https://bootswatch.com/5/flatly/bootstrap.min.css" %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <%= render 'layouts/navigation' %>
  <div class="container">
   <br/>
  <% if notice %>
    <%= render 'layouts/alerts' %>
    <% end %>

    

    <%= yield %>
    
  </div>
    
  </body>
</html>
